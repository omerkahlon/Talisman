public class Plague extends AdventureCard
{
    public Player P;
    public ArrayList<Player> Players;
    public Plague()
    {
        int PlayerRegion = P.GetCurrTile().GetRegion();
        for (Player player : Players) {
            int CurrentPlayerRegion = player.GetCurrTile().GetRegion();
            if (CurrentPlayerRegion == PlayerRegion) {
                player.LoseLifePoint();
            }
        }
    }
}
