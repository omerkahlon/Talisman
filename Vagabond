package com.example.talisman.AdventureCards;

import com.example.talisman.AdventureCard;
import com.example.talisman.Player;
import com.example.talisman.Dice;
import com.example.talisman.Item;
public class Vagabond extends AdventureCard
{
    public Vagabond()
    {
        super("Vagabond");
    }
    public void VagabondEncounter(Player Pl)
    {
        if (!Pl.HasTalisman())
        {
            Pl.AddItem(new Item("Talisman"));
        }
    }
}
